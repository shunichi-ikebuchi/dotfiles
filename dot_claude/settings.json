{
  "permissions": {
    "read": [
      "!../**",
      "./**",
      "!./node_modules/**",
      "!./.git/**"
    ],
    "edit": [
      "!../**",
      "./**/*.go",
      "./**/*.ts",
      "./**/*.js",
      "./**/*.py"
    ],
    "commands": {
      "safe-cmd": [
        "ls ./",
        "pwd",
        "cat ./",
        "grep ./",
        "git status",
        "git diff",
        "npm install --prefix ./",
        "yarn install --prefix ./",
        "Bash(gh:*)",
        "Bash(git:*)",
        "WebFetch(domain:github.com)",
        "WebFetch(domain:api.github.com)"
      ],
      "dangerous-cmd": [
        "!rm",
        "!sudo",
        "!curl",
        "!wget",
        "!cd ..",
        "!* ../*",
        "!* /home/*",
        "!* /etc/*",
        "!* /root/*"
      ]
    }
  },
  "ide": {
    "enabled": true,
    "autoShareErrors": true
  },
  "notifications": {
    "taskCompleteSound": true
  }
}
